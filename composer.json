{
    "name": "2lenet/hermes-bundle",
    "type": "symfony-bundle",
    "description": "Email sender library",
    "homepage": "https://www.2le.net",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lle\\HermesBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "2LE",
            "email": "contact@2le.net"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/orm-pack": "*",
        "symfony/framework-bundle": "^5.0|^6.0",
        "twig/twig": "^3.4",
        "doctrine/annotations": "^1.13",
        "doctrine/collections": "^1.0",
        "doctrine/common": "^2.13 || ^3.0",
        "doctrine/event-manager": "^1.0",
        "doctrine/persistence": "^1.3.4 || ^2.0 || ^3.0",
        "symfony/validator": "^5.3 || ^6.0",
        "symfony/dependency-injection": "^5.3 || ^6.0",
        "symfony/yaml": "^5.3 || ^6.0",
        "symfony/mailer": "^5.3 || ^6.0",
        "symfony/twig-bundle": "^5.3 || ^6.0",
        "symfony/lock": "^5.3 || ^6.0",
        "2lenet/crudit-bundle": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "tests/extension.neon"
            ]
        }
    }
}
